<!DOCTYPE html>
<html>
  <head>
    <title>SampleApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <heder>
      <h1 class="headline">All Lists</h1>
    </heder>
    <nav>
      <ul class="nav-list">
        <li class="nav-list-item">
          <%= link_to "Top",top_path %>
        </li>
        <li class="nav-list-item">
          <%= link_to "TodoLists", todolist_path %>
        </li>
        <li class="nav-list-item">
          <%= link_to "NewLists", todolists_new_path %>
        </li>
      </ul>
    </nav>
    <main>
      
      <%= yield %>
    </main>
    
  </body>
</html>
